 langid = {english},
 month = {January},
 note = {Good review of recent DL techniques on fault diagnosis and prognosis. Reviews CNN, DBN, RNN (\& LSTM), GAN approaches as well as transformer (time series transformer and vision transformer for fault classification) and graph neural networks (to better link relationships between variables). Says that Transformer and GANs are gradually improving and receiving more attention. Good review of current challenges},
 number = {3},
