 pages = {56},
 shorttitle = {Challenges and {{Opportunities}} of {{AI-Enabled Monitoring}}, {{Diagnosis}} \& {{Prognosis}}},
 title = {Challenges and {{Opportunities}} of {{AI-Enabled Monitoring}}, {{Diagnosis}} \& {{Prognosis}}: {{A Review}}},
