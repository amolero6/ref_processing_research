@article{liPerspectiveSurveyDeep2022,
  title = {A Perspective Survey on Deep Transfer Learning for Fault Diagnosis in Industrial Scenarios: {{Theories}}, Applications and Challenges},
  shorttitle = {A Perspective Survey on Deep Transfer Learning for Fault Diagnosis in Industrial Scenarios},
