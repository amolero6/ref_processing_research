@article{yinUNetLSTMTimeSeriesEnhanced2023,
  title = {U-{{Net-LSTM}}: {{Time Series-Enhanced Lake Boundary Prediction Model}}},
  shorttitle = {U-{{Net-LSTM}}},
  author = {Yin, Lirong and Wang, Lei and Li, Tingqiao and Lu, Siyu and Tian, Jiawei and Yin, Zhengtong and Li, Xiaolu and Zheng, Wenfeng},
