@article{farahaniTimeseriesClassificationSmart2025,
  title = {Time-Series Classification in Smart Manufacturing Systems: {{An}} Experimental Evaluation of State-of-the-Art Machine Learning Algorithms},
  shorttitle = {Time-Series Classification in Smart Manufacturing Systems},
  author = {Farahani, Mojtaba A. and McCormick, M.R. and Harik, Ramy and Wuest, Thorsten},
