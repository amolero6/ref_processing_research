@article{zhangFaultDiagnosisBidirectional2023,
 abstract = {In recent years, supervised deep learning methods have proven to be feasible and advanced for fault diagnosis. They do, however, rely on large amounts of accurately labeled data. It is challenging to accurately label incipient or secondary fault signals in the presence of operational background noise, resulting in noisy labels. This study presents a bidirectional guidance method that enables deep networks to diagnose faults robustly with the ability to label noise tolerance. First, the proposed method uses discrete wavelet packet transform (DWPT) to preprocess vibration signals before feeding them into the deep convolutional network. Second, a bidirectional loss (BL) function is proposed to significantly improve the robustness of the model in the case of noisy labels. Third, a fast cosine decay (FCD) strategy of the learning rate is designed to further boost the recognition performance of the model under severe noisy labels. The proposed method is applied to testbed experiments and wind turbine fault diagnosis. The experimental results demonstrate that it outperforms other cutting-edge methods in fault diagnosis with severely noisy labels.},
 author = {Zhang, Kai and Li, Zhixuan and Zheng, Qing and Ding, Guofu and Tang, Baoping and Zhao, Minghang},
 copyright = {https://ieeexplore.ieee.org/Xplorehelp/downloads/license-information/IEEE.html},
 doi = {10.1109/JSEN.2023.3289948},
 issn = {1530-437X, 1558-1748, 2379-9153},
 journal = {IEEE Sensors Journal},
 keywords = {2D input,Anomaly detection,Classified,CNN,Data handling techniques,Deep Learning,Fault classification,notion,Processed data,Read,RPlan: A},
 langid = {english},
 month = {August},
 note = {Fault diagnosis of turbine faults with noisy labels using CNN with vibration data and processed by discrete wavelet transform to 2D input. Uses bidirectional loss to increase robustness. Several adjustments to improve robustness that may lead to poor generalization},
 number = {16},
 pages = {18810--18820},
 title = {Fault {{Diagnosis With Bidirectional Guided Convolutional Neural Networks Under Noisy Labels}}},
 urldate = {2024-04-17},
 volume = {23},
 year = {2023}
}

@article{zhangTransformerFaultDiagnosis2020,
 abstract = {Transformer is important to the electric power systems, and its accurate fault diagnosis is still hard. In the paper, a novel transformer fault diagnosis method using an Internet of Things (IoT) based monitoring system and an ensemble machine learning (EML) is presented. The monitoring system based on IoT technology consists of two parts: a data measurement subsystem and a data reception subsystem. Firstly, transformer vibration signals are measured by using the data measurement subsystem, and they are sent to the remote server by using the data reception subsystem. Then, an EML composed of deep belief networks (DBNs) and stacked denoising autoencoders (SDAs) with different activation functions, and relevance vector machines (RVMs) is proposed. DBNs and SDAs are respectively used to extract features from the signals, and RVMs are respectively employed as classifier. In order to ensure efficient of the EML, a novel combination strategy is proposed. A transformer fault diagnosis experiment is performed, and the diagnosis results confirm that the designed monitoring system can collect vibration signals effectively and long-distance, and the proposed EML can usefully remedy the inadequate information of features extracted by individual deep learning method, and its RVM classifier is obviously better than other commonly used classifiers.},
 author = {Zhang, Chaolong and He, Yigang and Du, Bolun and Yuan, Lifen and Li, Bing and Jiang, Shanhe},
 doi = {10.1016/j.future.2020.03.008},
 issn = {0167739X},
 journal = {Future Generation Computer Systems},
 keywords = {1D input,Anomaly detection,Classified,DBN,Fault classification,notion,PCA visualization,Read,RPlan: A},
 langid = {english},
 month = {July},
 note = {Paper to detect and classify faults (over all classes) in transformers using DBN ensembled over acquired vibration signal data. Calculates diagnostic accuracy and evaluates using mean diagnostic accuracy. Implements it in an IoT monitoring device. Tests it over different fault modes of transformers},
 pages = {533--545},
 title = {Transformer Fault Diagnosis Method Using {{IoT}} Based Monitoring System and Ensemble Machine Learning},
 urldate = {2024-03-20},
 volume = {108},
 year = {2020}
}

@article{zhangTSViTTimeSeries,
 abstract = {Traditional fault diagnosis methods using Convolutional Neural Networks (CNNs) face limitations in capturing temporal features (i.e., the variation of vibration signals over time). To address this issue, this paper introduces a novel model, the Time Series Vision Transformer (TSViT), specifically designed for fault diagnosis. On one hand, TSViT model integrates a convolutional layer to segment vibration signals and capture local features. On the other hand, it employs a transformer encoder to learn long-term temporal information. The experimental results with other methods on two distinct datasets validate the effectiveness and generalizability of TSViT with a comparative analysis of its hyperparameters' impact on model performance, computational complexity, and overall parameter quantity. TSViT reaches average accuracies of 100\% and 99.99\% on two test sets, correspondingly.},
 author = {Zhang, Shouhua and Zhou, Jiehan and Ma, Xue and Wen, Chenglin and Pirttikangas, Susanna and Yu, Chen and Zhang, Weishan and Yang, Chunsheng},
 keywords = {1D input,Anomaly detection,Attention,bearings,Classified,Deep Learning,Good task theory (DL),Good theory explanation (CBM),High rated,Industrial data,Latent space,notion,Re-read,Read,RPlan: A,state of the art,t-SNE visualization,Testbench data,Transformer},
 langid = {english},
 note = {Good paper and similar to current task one. Uses conv + transformer in a vision transformer encoder. Divides sequence into patches (windows) and applies a conv layer before PE and the transformer encoder block. The output of the transformer encoder block goes to a Classification layer that has layer normalization, dropout, a linear and a softmax. Conv is used for feature extraction and transformer for sequence dependencies and context. Vibration data from test bench. Supervised fault classification},
 title = {{{TSViT}}: {{A Time Series Vision Transformer}} for {{Fault Diagnosis}}}
}

