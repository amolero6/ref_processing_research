 doi = {10.1049/rpg2.12016},
 issn = {1752-1424},
 journal = {IET Renewable Power Generation},
 keywords = {CNN,Deep Learning,Fault classification,Latent space,notion,Rplan: A},
 langid = {english},
 note = {CNN for fault classification on photovoltaic data. Simple approach, good results but some confusion between similar faults},
 number = {1},
 pages = {193--205},
 title = {Deep-Learning--Based Method for Faults Classification of {{PV}} System},
 urldate = {2024-03-20},
 volume = {15},
 year = {2021}
}

