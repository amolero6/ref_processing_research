@article{zhaoDenoisingDiffusionProbabilistic2025,
 abstract = {Bearing fault is one of the main causes of rotating machinery failure. However, collecting sufficient failure data proves challenging in real-world industrial environments due to their complexity. Owing to this constraint, the majority of current methods fail to accurately identify fault types with limited data, thereby impeding timely maintenance efforts. To address this issue, we propose a bearing fault diagnosis method utilizing diffusion model data enhancement in this study. Following data collection, we employ the continuous wavelet transform to convert various one-dimensional vibration data into two-dimensional time series graphs. Subsequently, these feature graphs are partitioned into sets of fault samples. Then, the diffusion model is employed to augment the training samples. These augmented samples are then fed into the convolutional neural network for fault diag\- nosis, and their diagnostic accuracy is compared with that of the original dataset. The comparative analysis demonstrates that the data augmentation technique, founded on the diffusion model, enhances fault diagnosis accuracy within a small sample training dataset. Ultimately, the efficacy of the proposed method is validated utilizing the Paderborn bearing dataset and juxtaposed against alternative data augmentation techniques. The findings indicate that the diffusion model-based data augmentation method outperforms other techniques in terms of both training accuracy and stability, particularly in scenarios with small sample.},
 author = {Zhao, Pengcheng and Zhang, Wei and Cao, Xiaoshan and Li, Xiang},
 doi = {10.1016/j.engappai.2024.109520},
 issn = {09521976},
 journal = {Engineering Applications of Artificial Intelligence},
 keywords = {2D input,Anomaly detection,bearings,Classified,CNN,Diffusion models,Fault classification,Good task theory (DL),notion,Oversampling,Processed data,Read,RPlan: A,state of the art,Testbench data},
 langid = {english},
 month = {January},
 note = {Paper that presents the use of diffusion models to enhance data generation and thus improve fault diagnosis. Focuses on diagnosis accuracy improvement using diffusion models that synthetically generate more data samples. Converts raw vibration signals of bearings to 2D using continuous wavelet transform as diffusion models seem to not work well with time-series data. Then uses U-net as the diffusion model to add and reduce noise for samples to generate a feature space that later will be fed to a CNN for fault diagnosis classification (fault type)},
 pages = {109520},
 title = {Denoising Diffusion Probabilistic Model-Enabled Data Augmentation Method for Intelligent Machine Fault Diagnosis},
 urldate = {2024-11-15},
 volume = {139},
 year = {2025}
}

@article{zhouFaultFormerPretrainingTransformers2024,
 abstract = {The growth of global consumption has motivated important applications of deep learning to smart manufacturing and machine health monitoring. In particular, analyzing vibration data offers great potential to extract meaningful insights into predictive maintenance by the detection of bearing faults. Deep learning can be a powerful method to predict these mechanical failures; however, they lack generalizability to new tasks or datasets and require expensive, labeled mechanical data. We address this by presenting a novel self-supervised pretraining and fine-tuning framework based on transformer models. In particular, we investigate different tokenization and data augmentation strategies to reach state-of-the-art accuracies using transformer models. Furthermore, we demonstrate self-supervised masked pretraining for vibration signals and its application to low-data regimes, task adaptation, and dataset adaptation. Pretraining is able to improve performance on scarce, unseen training samples, as well as when fine-tuning on fault classes outside of the pretraining distribution. Furthermore, pretrained transformers are shown to be able to generalize to a different dataset in a few-shot manner. This introduces a new paradigm where models can be pretrained on unlabeled data from different bearings, faults, and machinery and quickly deployed to new, data-scarce applications to suit specific manufacturing needs.},
 archiveprefix = {arXiv},
 author = {Zhou, Anthony and Farimani, Amir Barati},
 doi = {10.1109/ACCESS.2024.3399670},
 eprint = {2312.02380},
 issn = {2169-3536},
 journal = {IEEE Access},
 keywords = {1D input,Anomaly detection,Attention,bearings,Classified,CNN,Computer Science - Machine Learning,Deep Learning,Electrical Engineering and Systems Science - Signal Processing,Fault classification,Good task theory (DL),Good theory explanation (CBM),Good theory explanation (PHM),Industrial data,NN,notion,Re-read,Read,RPlan: A,state of the art,To read for paper,Transformer},
 langid = {english},
 note = {Comment: 10 pages, 6 figures
\par
Great paper and similar to our task. Uses transformer (Encoder) to learn features and then classify faults from the encoder output. Uses class token and tokenization. Compares between different models using public bearing dataset (but not against state of the art). Great problem statement and explanation of the task as well as good references for the task. Really good explanation of each transformer layer and architecture.},
 pages = {70719--70728},
