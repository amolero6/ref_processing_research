@article{hanRollingBearingFault2021,
 abstract = {For small sample data, it is difficult to complete the requirements of training complex models in the field of fault diagnosis. To solve the problem, this paper combines convolutional neural network's excellent feature processing ability with the excellent generalization ability of Support Vector Machine (SVM). The proposed CNN-SVM system is applied in bearing fault diagnosis, which takes the time domain diagram of bearing vibration data as the system input. The features are extracted by CNN, and realizes the final bearing state recognition by SVM. The contribution of the paper is to add three conditions for automatically switch CNN to SVM. The results show that the system has the advantages of less time-consuming, high precision and strong generalization ability. Experimental results show that the time consumption of this model is 1/3 of CNN, and the accuracy of the training set and the testing set are 100\% and 99.44\%.},
 author = {Han, Tian and Zhang, Longwen and Yin, Zhongjun and Tan, Andy C.C.},
 doi = {10.1016/j.measurement.2021.109022},
 issn = {02632241},
 journal = {Measurement},
 keywords = {1D input,Anomaly detection,CNN,Deep Learning,Fault classification,Historical Data,Latent space,notion,Processed data,RPlan: A,SCADA data},
 langid = {english},
 month = {June},
 note = {CNN + SVM. Uses CNN for feature extraction first and anomaly detection and then SVM for fault classification of known faults},
 pages = {109022},
 title = {Rolling Bearing Fault Diagnosis with Combined Convolutional Neural Networks and Support Vector Machine},
 urldate = {2024-03-06},
 volume = {177},
 year = {2021}
}

@article{huangMetricLearningBasedFault2024,
 abstract = {Industrial system monitoring includes fault diagnosis and anomaly detection, which have received extensive attention, since they can recognize the fault types and detect unknown anomalies. However, a separate fault diagnosis method or anomaly detection method cannot identify unknown faults and distinguish between different fault types simultaneously; thus, it is difficult to meet the increasing demand for safety and reliability of industrial systems. Besides, the actual system often operates in varying working conditions and is disturbed by the noise, which results in the intraclass variance of the raw data and degrades the performance of industrial system monitoring. To solve these problems, a metric learning-based fault diagnosis and anomaly detection method is proposed. Fault diagnosis and anomaly detection are adaptively fused in the proposed endto-end model, where anomaly detection can prevent the model from misjudging the unknown anomaly as the known type, while fault diagnosis can identify the specific type of system fault. In addition, a novel multicenter loss is introduced to restrain the intraclass variance. Compared with manual feature extraction that can only extract suboptimal features, it can learn discriminant features automatically for both fault diagnosis and anomaly detection tasks. Experiments on three-phase flow (TPF) facility and Case Western Reserve University (CWRU) bearing have demonstrated that the proposed method can avoid the interference of intraclass variances and learn features that are effective for identifying tasks. Moreover, it achieves the best performance in both fault diagnosis and anomaly detection.},
 author = {Huang, Keke and Wu, Shujie and Sun, Bei and Yang, Chunhua and Gui, Weihua},
 doi = {10.1109/TNNLS.2022.3175888},
 issn = {2162-237X, 2162-2388},
 journal = {IEEE Transactions on Neural Networks and Learning Systems},
 keywords = {Anomaly detection,Deep Learning,Fault classification,Latent space,NN,notion,RPlan: A},
 langid = {english},
 month = {January},
 note = {Combines anomaly detection + fault classification. Uses a loss to reduce intraclass variance and maximize interclass distance. Uses t-SNE for latent space analysis},
 number = {1},
 pages = {547--558},
 title = {Metric {{Learning-Based Fault Diagnosis}} and {{Anomaly Detection}} for {{Industrial Data With Intraclass Variance}}},
 urldate = {2024-03-08},
 volume = {35},
 year = {2024}
}

